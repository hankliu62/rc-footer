{
  "name": "@hankliu/rc-footer",
  "version": "0.0.1",
  "description": "React easy to use footer component",
  "keywords": [
    "react",
    "react-component",
    "react-footer",
    "footer"
  ],
  "main": "./lib/index",
  "module": "./es/index",
  "types": "./lib/index.d.ts",
  "files": [
    "assets/*.css",
    "es",
    "lib"
  ],
  "homepage": "https://github.com/hankliu62/rc-footer",
  "repository": {
    "type": "git",
    "url": "git@github.com:hankliu62/rc-footer.git"
  },
  "bugs": {
    "url": "http://github.com/hankliu62/rc-footer/issues"
  },
  "license": "MIT",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "compile": "father build && lessc assets/index.less assets/index.css",
    "prepublishOnly": "npm run compile && np --yolo --no-publish --any-branch",
    "lint": "eslint src/ --ext .ts,.tsx,.jsx,.js,.md",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "rc-test -u",
    "coverage": "rc-test -u --coverage",
    "now-build": "npm run docs:build"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.4",
    "@rc-component/portal": "^1.1.2",
    "classnames": "^2.5.1",
    "rc-tooltip": "~5.2.2"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.3.6",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@types/classnames": "^2.3.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.78",
    "@types/react-dom": "^18.2.25",
    "@umijs/fabric": "^4.0.1",
    "cross-env": "^7.0.3",
    "dumi": "^2.2.17",
    "eslint": "^9.0.0",
    "father": "^4.4.0",
    "glob": "^10.3.12",
    "less": "^4.2.0",
    "np": "^10.0.3",
    "rc-test": "^7.0.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.4.5"
  }
}
